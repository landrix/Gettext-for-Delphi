msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:62
msgid "    if (s != null)"
msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:63
msgid "      document.F.S.value = s;"
msgstr ""

