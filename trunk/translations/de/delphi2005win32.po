msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:50
msgid "    for (var i=0; i <aCookie.length; i++)"
msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:51
msgid "    {"
msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:52
msgid "      var aCrumb = aCookie[i].split(\"=\");"
msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:53
msgid "      if (sName == aCrumb[0])"
msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:54
msgid "        return unescape(aCrumb[1]);"
msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:55
msgid "    }"
msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:56
msgid "    return null;"
msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:59
msgid "  function init()"
msgstr ""

# Label: ignore
#. WebModule2..PageProducerSelect....HTMLDoc.Strings
#: SvrInfoModule.dfm:61
