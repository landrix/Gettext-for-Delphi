#. Programmer's name for it: SDuplicateDatabaseName
#: dbisamtb.pas:30
msgid "Database name %s already exists."
msgstr "Дати назив за базу података '%s'  већ постоји"

#. Programmer's name for it: SDuplicateSessionName
#: dbisamtb.pas:31
msgid "Session name %s already exists."
msgstr "Сесија са називом '%s'  већ постоји"

#. Programmer's name for it: SInvalidSessionName
#: dbisamtb.pas:32
msgid "Session name %s is not valid."
msgstr "Назив сесије '%s'  није валидан"

#. Programmer's name for it: SDatabaseNameMissing
#: dbisamtb.pas:33
msgid "Database name is missing."
msgstr "Недостаје назив абзе података."

#. Programmer's name for it: SInvalidDatabaseName
#: dbisamtb.pas:34
msgid "Database name %s is not valid."
msgstr "Назива базе података '%s'  није валидан."

#. Programmer's name for it: SSessionNameMissing
#: dbisamtb.pas:35
msgid "Session name is missing."
msgstr "Недостаје назив сесије."

#. Programmer's name for it: SDatabaseOpen
#: dbisamtb.pas:36
msgid "Cannot perform this operation on an open database."
msgstr "Операција није могућа над отвореном базом података."

#. Programmer's name for it: SDatabaseClosed
#: dbisamtb.pas:37
msgid "Cannot perform this operation on a closed database."
msgstr "Операција није могућа над затвореном базом података."

#. Programmer's name for it: SDatabaseHandleSet
#: dbisamtb.pas:38
msgid "Database handle owned by a different session"
msgstr "Вредност handle базе података је у власништву друге сесије"

#. Programmer's name for it: SSessionActive
#: dbisamtb.pas:39
msgid "Cannot perform this operation on an active session."
msgstr "Операција није могућа над активном сесијом"

#. Programmer's name for it: SAutoSessionActive
#: dbisamtb.pas:40
msgid "Cannot perform this operation on the active auto-session."
msgstr "Операција није могућа над активном ауто-сесијом"

#. Programmer's name for it: SAutoSessionExclusive
#: dbisamtb.pas:41
msgid "The active auto-session can be the only active session."
msgstr "Активна ауто-сесија може бити једина активна сесија"

#. Programmer's name for it: SAutoSessionExists
#: dbisamtb.pas:42
msgid "This auto-session already exists."
msgstr "Ова ауто-сесија већ постоји."

#. Programmer's name for it: SHandleError
#: dbisamtb.pas:43
msgid "Error creating table handle."
msgstr "Грешка приликом креирања handle. вредности табеле."

#. Programmer's name for it: SFieldUndefinedType
#: dbisamtb.pas:44
msgid "Field '%s' is of unknown type"
msgstr "Поље '%s' је непознатог типа"

#. Programmer's name for it: SFieldUnsupportedType
#: dbisamtb.pas:45
msgid "Field '%s' is of an unsupported type"
msgstr "Поље '%s' је типа који није подржан"

#. Programmer's name for it: SEmptySQLStatement
#: dbisamtb.pas:46
msgid "No SQL statement available"
msgstr "Нема доступног SQL упита"

#. Programmer's name for it: SNoParameterValue
#: dbisamtb.pas:47
msgid "No value for parameter '%s'"
msgstr "Нема вредности за параметар '%s' "

#. Programmer's name for it: SNoParameterType
#: dbisamtb.pas:48
msgid "No parameter type for parameter '%s'"
msgstr "Недостаје тип параметра за параметар '%s' "

#. Programmer's name for it: SParameterNotFound
#: dbisamtb.pas:49
msgid "Parameter '%s' not found"
msgstr "Параметар '%s' није нађен"

#. Programmer's name for it: SParamTooBig
#: dbisamtb.pas:50
msgid "Parameter '%s', cannot save data larger than %d bytes"
msgstr "Параметар '%s', није могуће снимање података већих од %d б"

#. Programmer's name for it: SNotEditing
#: dbisamtb.pas:51
msgid "Table is not in edit or insert mode."
msgstr "Табела није у моду за измене или додавање редова.."

#. Programmer's name for it: STableMismatch
#: dbisamtb.pas:52
msgid "Source and destination tables are incompatible."
msgstr "Изворна и циљна табела нису компатибилне."

#. Programmer's name for it: SFieldReadOnly
#: dbisamtb.pas:53
msgid "Field %s cannot be modified."
msgstr "Поље '%s' није могуће модификовати"

#. Programmer's name for it: SNotIndexField
#: dbisamtb.pas:54
msgid "Field %s is not in the current index."
msgstr "Поље '%s' није део тренутог индекса"

#. Programmer's name for it: SCircularDataLink
#: dbisamtb.pas:55
msgid "Circular data links are not allowed."
msgstr "Циркуларне везе података нису дозвољене."

#. Programmer's name for it: SFieldIndexError
#: dbisamtb.pas:56
msgid "Field index out of range."
msgstr "Индекс поља је ван опсега."

#. Programmer's name for it: SNoFieldIndexes
#: dbisamtb.pas:57
msgid "No fields defined for the current index."
msgstr "Нема поља дефинисаних за тренутни индекс."

#. Programmer's name for it: SIndexFieldMissing
#: dbisamtb.pas:58
msgid "Cannot access index field %s."
msgstr "Приступ индекс пољу %s није успео."

#. Programmer's name for it: SBeginTransError
#: dbisamtb.pas:59
msgid "Database %s already has an active transaction."
msgstr "База података '%s' већ има активну трансакцију."

#. Programmer's name for it: SEndTransError
#: dbisamtb.pas:60
msgid "Database %s has no active transaction."
msgstr "База података '%s' нема активну трансакцију"

#. Programmer's name for it: SNoTableName
#: dbisamtb.pas:61
msgid "Missing table name."
msgstr "Недостаје назив табеле."

#. Programmer's name for it: SNoFieldAccess
#: dbisamtb.pas:62
msgid "Cannot access field %s."
msgstr "Није успео приступ пољу %s."

#. Programmer's name for it: SInvalidVersion
#: dbisamtb.pas:63
msgid "Unable to load bind parameters"
msgstr "Није успело учитавање параметара за повезивање"

#. Programmer's name: DBISAM_ERRSTR_NOTINITIALIZED
#: dbisamer.rc:12
msgid "The database engine is not initialized"
msgstr "Библиотека за рад са базом података није иницијализована"

#. Programmer's name: DBISAM_ERRSTR_OSUNKNOWN
#: dbisamer.rc:13
msgid "An unknown operating system error occurred with the table '%s'"
msgstr "Догодила се непозната грешка оперативног система за табелу '%s'"

#. Programmer's name: DBISAM_ERRSTR_OSENOMEM
#: dbisamer.rc:14
msgid "There is insufficient operating system memory available for the current operation"
msgstr "Нема довољно меморије за тренутну акцију"

#. Programmer's name: DBISAM_ERRSTR_OSENOENT
#: dbisamer.rc:15
msgid "Table '%s' does not exist"
msgstr "Табела '%s' не постоји"

#. Programmer's name: DBISAM_ERRSTR_OSEACCES
#: dbisamer.rc:16
msgid "Access denied to table '%s'"
msgstr "Приступ табели није дозвољен '%s'"

#. Programmer's name: DBISAM_ERRSTR_OSENODEV
#: dbisamer.rc:17
msgid "Access denied to logical operating system device for the table '%s'"
msgstr "Приступ логичком уређају оперативног система није дозвољен за табелу '%s'"

#. Programmer's name: DBISAM_ERRSTR_FILEDELETEFAIL
#: dbisamer.rc:18
msgid "Cannot delete the table '%s'"
msgstr "Брисање табеле није успело '%s'"

#. Programmer's name: DBISAM_ERRSTR_FILENOACCESS
#: dbisamer.rc:19
msgid "Error accessing header information for the table '%s'"
msgstr "Грешка приликом притупа информацијама у заглављу табеле '%s'"

#. Programmer's name: DBISAM_ERRSTR_LOCKED
#: dbisamer.rc:20
msgid "The table '%s' is already locked by another user"
msgstr "Табела '%s' је већ закључана од стране другог корисника."

#. Programmer's name: DBISAM_ERRSTR_LOCKTIMEOUT
#: dbisamer.rc:21
msgid "General lock failure with the table '%s'"
msgstr "Грешка приликом закључавања табеле '%s'"

#. Programmer's name: DBISAM_ERRSTR_UNLOCKFAILED
#: dbisamer.rc:22
msgid "Cannot unlock table or record in the table '%s'"
msgstr "Није успело откључавање реда у табели '%s'"

#. Programmer's name: DBISAM_ERRSTR_TABLEOPEN
#: dbisamer.rc:23
msgid "The table '%s' is already in use"
msgstr "Табела '%s' је већ у употреби"

#. Programmer's name: DBISAM_ERRSTR_READERR
#: dbisamer.rc:24
msgid "Error reading from the table '%s'"
msgstr "Грешка приликом читања из табеле '%s'"

#. Programmer's name: DBISAM_ERRSTR_WRITEERR
#: dbisamer.rc:25
msgid "Error writing to the table '%s'"
msgstr "Грешка приликом уписивања у табелу '%s' "

#. Programmer's name: DBISAM_ERRSTR_RECLOCKFAILED
#: dbisamer.rc:26
msgid "Record in the table '%s' is in use by another user"
msgstr "Ред у табели %s је у употреби од стране другог корисника."

#. Programmer's name: DBISAM_ERRSTR_OSEMFILE
#: dbisamer.rc:27
msgid "Too many operating system files open while attempting to open the table '%s'"
msgstr "Сувише фајлова оперативног система је отворено приликом отварања табеле '%s'"

#. Programmer's name: DBISAM_ERRSTR_OSEBADF
#: dbisamer.rc:28
msgid "Invalid operating system file handle for the table '%s'"
msgstr "Вредност handle фајла табеле '%s' није валидна"

#. Programmer's name: DBISAM_ERRSTR_OSENOTSAM
#: dbisamer.rc:29
msgid "The table '%s' has been moved"
msgstr "Табела '%s' је премештена"

#. Programmer's name: DBISAM_ERRSTR_OLDVERSION
#: dbisamer.rc:30
msgid "The table '%s' is not the correct version"
msgstr "Табела '%s' није исправне верзије"

#. Programmer's name: DBISAM_ERRSTR_KEYVIOL
#: dbisamer.rc:31
msgid "Duplicate key found in the index '%s' of the table '%s'"
msgstr "Дупли кључ нађен у индексу '%s' табеле '%s'"

#. Programmer's name: DBISAM_ERRSTR_KEYORRECDELETED
#: dbisamer.rc:32
msgid "Record has been changed or deleted by another user, session, or table cursor in the table '%s'"
msgstr "Ред је измењен или обрисан од стране другог корисника, сесије или курсора табеле за табелу '%s'"

#. Programmer's name: DBISAM_ERRSTR_RECTOOBIG
#: dbisamer.rc:33
msgid "Maximum record size exceeded in the table '%s'"
msgstr "Премашена је максимална дужина реда у табели '%s'"

#. Programmer's name: DBISAM_ERRSTR_FLDLIMIT
#: dbisamer.rc:34
msgid "Maximum or minimum limits on number of fields, auto-increment field value, or field size exceeded in the table '%s'"
msgstr "Премашен је максимум или минимум броја поља, вредност auto-inc поља, или величина поља табеле '%s'"

#. Programmer's name: DBISAM_ERRSTR_INDEXLIMIT
#: dbisamer.rc:35
msgid "Maximum or minimum limits on number of indexes or fields in an index exceeded in the table '%s'"
msgstr "Премашен је максимум или минимум броја индекса или поља у индексу табеле '%s'"

#. Programmer's name: DBISAM_ERRSTR_RECNOTFOUND
#: dbisamer.rc:36
msgid "Record not found in the table '%s'"
msgstr "Ред није нађен у табели '%s'"

#. Programmer's name: DBISAM_ERRSTR_NEEDEXCLACCESS
#: dbisamer.rc:37
msgid "The table '%s' must be opened exclusively"
msgstr "Табела '%s' мора бити отворена у екслузивном моду"

#. Programmer's name: DBISAM_ERRSTR_HEADERCORRUPT
#: dbisamer.rc:38
msgid "Header information corrupt in the table '%s'"
msgstr "Заглавље табеле '%s' је оштећено "

#. Programmer's name: DBISAM_ERRSTR_FILECORRUPT
#: dbisamer.rc:39
msgid "Data record buffers corrupt in the table '%s'"
msgstr "Бафери са подацима су оштећени у табели '%s'"

#. Programmer's name: DBISAM_ERRSTR_INDEXCORRUPT
#: dbisamer.rc:40
msgid "Index page buffers corrupt in the table '%s'"
msgstr "Индекс страна  је оштећена у табели '%s'"

#. Programmer's name: DBISAM_ERRSTR_MEMOCORRUPT
#: dbisamer.rc:41
msgid "BLOB block buffers corrupt in the table '%s'"
msgstr "BLOB блок бафер је оштећен у табели '%s'"

#. Programmer's name: DBISAM_ERRSTR_OUTOFRANGE
#: dbisamer.rc:42
msgid "Invalid field number or name '%s' specified for the table '%s'"
msgstr "Редни број или назив поља '%s' табеле '%s' није валидан"

#. Programmer's name: DBISAM_ERRSTR_INVALIDKEY
#: dbisamer.rc:43
msgid "Invalid key size specified in the index '%s' for the table '%s'"
msgstr "Дужина кључа дата у индексу '%s' табеле '%s' није валидна"

#. Programmer's name: DBISAM_ERRSTR_REQDERR
#: dbisamer.rc:44
msgid "A value must be provided for the field '%s' in the table '%s'"
msgstr "Поље '%s' табеле '%s' мора имати вредност"

#. Programmer's name: DBISAM_ERRSTR_MINVALERR
#: dbisamer.rc:45
msgid "The value for the field '%s' in the table '%s' is below the minimum allowable value"
msgstr "Вредност поља '%s' табеле '%s' је мања од минимално дозвољене"

#. Programmer's name: DBISAM_ERRSTR_MAXVALERR
#: dbisamer.rc:46
msgid "The value for the field '%s' in the table '%s' is above the maximum allowable value"
msgstr "Вредност поља '%s' табеле '%s' је већа од маскимално дозвољене"

#. Programmer's name: DBISAM_ERRSTR_INVALIDFLDTYPE
#: dbisamer.rc:47
msgid "Invalid field definition specified for the field '%s' in the table '%s'"
msgstr "Дефиниција поља '%s' табеле '%s' није валидна"

#. Programmer's name: DBISAM_ERRSTR_SEARCHCOLREQD
#: dbisamer.rc:48
msgid "Invalid field '%s' specified for the index '%s' in the table '%s'"
msgstr "Поље '%s' дато за индекс '%s' табеле '%s' није валидно"

#. Programmer's name: DBISAM_ERRSTR_INVALIDBLOBLEN
#: dbisamer.rc:49
msgid "Invalid blob length in the table '%s'"
msgstr "Дужина blob поља није валидна у табели '%s'"

#. Programmer's name: DBISAM_ERRSTR_NOTABLOB
#: dbisamer.rc:50
msgid "Invalid blob field '%s' specified for the table '%s'"
msgstr "blob поље '%s' дато за тебалу '%s' ниоје валидно"

#. Programmer's name: DBISAM_ERRSTR_ENDOFBLOB
#: dbisamer.rc:51
msgid "End of blob data in the table '%s' reached prematurely"
msgstr "Крај blob података табеле '%s' је прерано достигнут"

#. Programmer's name: DBISAM_ERRSTR_INVALIDBLOBOFFSET
#: dbisamer.rc:52
msgid "Invalid blob offset into the table '%s' specified"
msgstr "Офсет вредност blob поља дато за табелу '%s' није валидна"

#. Programmer's name: DBISAM_ERRSTR_INVALIDBLOBHANDLE
#: dbisamer.rc:53
msgid "Invalid blob handle for the table '%s' specified"
msgstr "Вредност handle blob поља табеле '%s' није валидна"

#. Programmer's name: DBISAM_ERRSTR_OPENBLOBLIMIT
#: dbisamer.rc:54
msgid "Too many blobs opened in the table '%s'"
msgstr "Превише отвпрених blob поља у табели '%s'"

#. Programmer's name: DBISAM_ERRSTR_BLOBLIMIT
#: dbisamer.rc:55
msgid "Too many blob fields or invalid blob block size specified for the table '%s'"
msgstr "Превише blob поља или иеисправна вличина блока дата за табелу  '%s'"

#. Programmer's name: DBISAM_ERRSTR_BOF
#: dbisamer.rc:56
msgid "Beginning of table '%s' unexpectedly encountered"
msgstr "Почетак табеле '%s' је неочекивано нађена"

#. Programmer's name: DBISAM_ERRSTR_EOF
#: dbisamer.rc:57
msgid "End of table '%s' unexpectedly encountered"
msgstr "Крај табеле '%s' на неочекиваном месту"

#. Programmer's name: DBISAM_ERRSTR_PRIMARYKEYREDEFINE
#: dbisamer.rc:58
msgid "A primary index is already defined for the table '%s' and cannot be added again"
msgstr "Примарни индекс је већ дефинисан за табелу '%s' и није га могуће додати опет"

#. Programmer's name: DBISAM_ERRSTR_INVALIDINDEXDELETE
#: dbisamer.rc:59
msgid "The primary index cannot be removed from the table '%s'"
msgstr "Примарни индекс није могуће уклонити из табеле '%s'"

#. Programmer's name: DBISAM_ERRSTR_INDEXNAMEREQUIRED
#: dbisamer.rc:60
msgid "The secondary index name is missing or not specified for the table '%s'"
msgstr "Назив секундарног индекса недостаје или није дат за табелу '%s'"

#. Programmer's name: DBISAM_ERRSTR_INDEXEXISTS
#: dbisamer.rc:61
msgid "The secondary index '%s' already exists for the table '%s'"
msgstr "Секундарни индекс '%s' већ постоји за табелу '%s'"

#. Programmer's name: DBISAM_ERRSTR_INVALIDINDEXNAME
#: dbisamer.rc:62
msgid "Invalid secondary index name '%s' specified for the table '%s'"
msgstr "Назив секундарног индекса '%s' табеле '%s' није валидан"

#. Programmer's name: DBISAM_ERRSTR_INVALIDIDXDESC
#: dbisamer.rc:63
msgid "Invalid field number '%s' specified in the index '%s' for the table '%s'"
msgstr "Редни број поља '%s' дат у индексу '%s' табеле '%s' није валидан"

#. Programmer's name: DBISAM_ERRSTR_INVALIDFILTER
#: dbisamer.rc:64
msgid "Filter error for the table '%s' - %s"
msgstr "Филтер грешка за табелу '%s'  - %s"

#. Programmer's name: DBISAM_ERRSTR_NOSUCHFILTER
#: dbisamer.rc:65
msgid "Invalid filter handle specified for the table '%s'"
msgstr "Вредност handle дата за табелу '%s' није валидна"

#. Programmer's name: DBISAM_ERRSTR_INTERNALLIMIT
#: dbisamer.rc:66
msgid "Too many filters defined for the table '%s'"
msgstr "Сувише филтера је дефинисано за табелу '%s'."

#. Programmer's name: DBISAM_ERRSTR_INVALIDFIELDNAME
#: dbisamer.rc:67
msgid "Invalid or duplicate field name '%s' specified for the table '%s'"
msgstr "Назив поља '%s' табеле '%s' није валидан или  или већ постоји"

#. Programmer's name: DBISAM_ERRSTR_INVALIDVCHKSTRUCT
#: dbisamer.rc:68
msgid "Invalid default, min or max validity check, or expression specified for the field '%s' in the table '%s'"
msgstr "Подразумевана, минимална или макисмална провера вредности , или израз дат за поље '%s' табеле '%s' није влидна "

#. Programmer's name: DBISAM_ERRSTR_NOTSUFFTABLERIGHTS
#: dbisamer.rc:69
msgid "Insufficient rights to the table '%s', a password is required"
msgstr "Немате права приступа табели '%s', потребна вам је шифра"

#. Programmer's name: DBISAM_ERRSTR_INVALIDPASSWORD
#: dbisamer.rc:70
msgid "Password provided for the table '%s' is invalid"
msgstr "Шифра дата за табелу '%s'  није валидна"

#. Programmer's name: DBISAM_ERRSTR_OSEINVMEM
#: dbisamer.rc:71
msgid "Invalid length for the in-memory table '%s'"
msgstr "Дужина меморијске табеле '%s' није валидна"

#. Programmer's name: DBISAM_ERRSTR_CANNOTLOADLDDRV
#: dbisamer.rc:72
msgid "The language driver for the table '%s' is not available or installed"
msgstr "Језички управљач табеле '%s' није доступан или није инсталиран"

#. Programmer's name: DBISAM_ERRSTR_SQLPARSE
#: dbisamer.rc:73
msgid "SQL error - %s"
msgstr "SQL грешка - %s"

#. Programmer's name: DBISAM_ERRSTR_TABLEFULL
#: dbisamer.rc:74
msgid "The table '%s' is full and cannot contain any more data"
msgstr "Табела '%s'  је попуњена и није могуће додавање нових података"

#. Programmer's name: DBISAM_ERRSTR_NOCURRREC
#: dbisamer.rc:75
msgid "No current record in the table '%s'"
msgstr "Нема тренутног реда у табели '%s' "

#. Programmer's name: DBISAM_ERRSTR_TABLEEXISTS
#: dbisamer.rc:76
msgid "The table '%s' already exists"
msgstr "Табела '%s'  већ постоји"

